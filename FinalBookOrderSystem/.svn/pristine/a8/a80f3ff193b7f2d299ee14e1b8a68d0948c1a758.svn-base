DROP TABLE BOOKSTORE_SECTION_BOOK;
DROP TABLE BOOKSTORE_SECTION;
DROP TABLE BOOKSTORE_PROFESSOR;
DROP TABLE BOOKSTORE_COURSE;
DROP TABLE BOOKSTORE_BOOK;
 
 CREATE TABLE "BOOKSTORE_BOOK" 
   (	"ISBN" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"TITLE" VARCHAR2(20 BYTE), 
	"EDITION" NUMBER, 
	"AUTHOR_F_NAME" VARCHAR2(20 BYTE), 
	"AUTHOR_L_NAME" VARCHAR2(20 BYTE), 
	"PUBLISHER" VARCHAR2(20 BYTE), 
	"PUBLISH_DATE" DATE, 
	"COST" NUMBER,
	 CONSTRAINT "BOOKSTORE_BOOK_PK" PRIMARY KEY ("ISBN")
   ); 

CREATE TABLE "BOOKSTORE_COURSE" 
   (	"COURSE_ID" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"COURSE_NAME" VARCHAR2(20 BYTE), 
	 CONSTRAINT "BOOKSTORE_COURSE_PK" PRIMARY KEY ("COURSE_ID")
   ); 

CREATE TABLE "BOOKSTORE_PROFESSOR" 
   (	"EMP_ID" VARCHAR2(25 BYTE) NOT NULL ENABLE, 
	"F_NAME" VARCHAR2(25 BYTE), 
	"L_NAME" VARCHAR2(25 BYTE), 
	"PROG_CHAIR" VARCHAR2(25 BYTE), 
  "EMAIL" VARCHAR2(25 BYTE), 
  "CAMPUS" VARCHAR2(25 BYTE), 
	 CONSTRAINT "PROFESSOR_PK_EMPID" PRIMARY KEY ("EMP_ID")
   ); 

CREATE TABLE "BOOKSTORE_SECTION" 
   (	"SECTION_ID" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"COURSE_ID" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"EMP_ID" VARCHAR2(20 BYTE), 
	"START_DATE" DATE, 
	"END_DATE" DATE, 
  "TIME" DATE,
  "TERM" VARCHAR2(20 BYTE), 
	 CONSTRAINT "BOOKSTORE_SECTION_PK" PRIMARY KEY ("SECTION_ID", "COURSE_ID"),
	 CONSTRAINT "BOOKSTORE_SECTION_BOOKSTO_FK1" FOREIGN KEY ("COURSE_ID")
	  REFERENCES "BOOKSTORE_COURSE" ("COURSE_ID") ENABLE, 
	 CONSTRAINT "BOOKSTORE_SECTION_BOOKSTO_FK2" FOREIGN KEY ("EMP_ID")
	  REFERENCES "BOOKSTORE_PROFESSOR" ("EMP_ID") ENABLE
   );

CREATE TABLE "BOOKSTORE_SECTION_BOOK" 
   ("REQ_ID" NUMBER NOT NULL ENABLE,
  "ISBN" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"COURSE_ID" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"SECTION_ID" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"QUANTITY" NUMBER, 
	"STATUS" VARCHAR2(20 BYTE), 
	 CONSTRAINT "BOOKSTORE_SECTION_BOOK_PK" PRIMARY KEY ("REQ_ID"),
	 CONSTRAINT "BOOKSTORE_SECTION_BOOK_BO_FK1" FOREIGN KEY ("ISBN")
	  REFERENCES "BOOKSTORE_BOOK" ("ISBN") ENABLE, 
	 CONSTRAINT "BOOKSTORE_SECTION_BOOK_BO_FK2" FOREIGN KEY ("SECTION_ID", "COURSE_ID")
	  REFERENCES "BOOKSTORE_SECTION" ("SECTION_ID", "COURSE_ID") ENABLE
   );

